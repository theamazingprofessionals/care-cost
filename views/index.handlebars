

<div class = "container">
	<button type = "button" class = "btn btn-default" id = "try">Test</button>
	<div class = "row" id = "what">
		<script id = "patient-data" type="text/x-handlebars-template">
		
			<ol>
				{{#each data}}
					<li>
						<p>{{this.name}}</p></li>
				{{/each}}
			</ol>

		</script>
	</div>
</div>



<header>
    <h1 class="text-center">This is the main page</h1>
</header>


<div id="list-div">
    <ul>
        {{#each names}}
        <li><button class="btn btn-secondary btn-sm proc_id" data-id="{{this.procedureId}}">{{this.procedureName}}</button></li>
        {{/each}}
    </ul>
</div>
<script src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/4.0.11/handlebars.js" type="text/javascript" charset="utf-8"></script>
